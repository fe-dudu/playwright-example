{
  "name": "playwright-example",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky",
    "dev": "dotenv -e .env npx playwright test",
    "dev:headed": "dotenv -e .env npx playwright test --headed",
    "dev:report": "dotenv -e .env npx playwright test --trace on --reporter=html",
    "playwright:report": "npx playwright test --trace on --reporter=html"
  },
  "dependencies": {
    "@playwright/test": "^1.49.1",
    "dotenv": "^16.4.7",
    "playwright": "^1.49.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "packageManager": "pnpm@9.6.0"
}
